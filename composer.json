{
  "name": "iquesters/foundation",
  "description": "A foundational package providing base classes and utilities for Laravel modules",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "laravel/framework": "^10.0|^11.0|^12.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Iquesters\\Foundation\\": "src/",
      "Iquesters\\Foundation\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "authors": [
    {
      "name": "Pritam Bag",
      "email": "pritam02b@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Iquesters\\Foundation\\FoundationServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}